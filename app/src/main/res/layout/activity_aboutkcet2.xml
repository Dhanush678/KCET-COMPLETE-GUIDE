<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/layoutMAIN"
    tools:context=".MainActivity2"
    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/back5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="15dp"
            android:src="@drawable/ic_baseline_arrow_back_24" />

        <TextView
            android:layout_width="329dp"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="53dp"
            android:layout_marginEnd="19dp"
            android:fontFamily="@font/arya"
            android:gravity="center_vertical"
            android:text="K-cet Exam Details"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />


    </RelativeLayout>


    <LinearLayout

        android:layout_width="match_parent"

        android:layout_height="match_parent"
        android:background="#00ff0000"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"

            android:layout_height="wrap_content"/>








        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="50dp"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginBottom="50dp"
                >


                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_marginStart="20dp"
                        android:background="@drawable/box">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:src="@drawable/download"
                            android:padding="5dp">

                        </ImageView>




                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/capriola"
                            android:padding="10dp"
                            android:layout_alignParentBottom="true"
                            android:paddingStart="30dp"
                            android:text="Application"
                            android:gravity="center"

                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                        <Button
                            android:id="@+id/application"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentBottom="true"
                            android:background="#00ff0000"
                            android:padding="20dp"

                            android:textAllCaps="true"
                            android:textColor="@color/blackTextColor" />


                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_marginStart="25dp"
                        android:background="@drawable/box">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:src="@drawable/pngwingcom"
                            android:padding="5dp">

                        </ImageView>




                        <TextView
                            android:id="@+id/syllabus1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/capriola"
                            android:layout_alignParentBottom="true"
                            android:text=" 1st Pu Syllabus"
                            android:gravity="center"
                            android:paddingStart="10dp"
                            android:layout_marginBottom="30dp"

                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="14sp" />
                        <TextView
                            android:id="@+id/syllabus2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/capriola"
                            android:layout_alignParentBottom="true"
                            android:text=" 2st Pu Syllabus"
                            android:gravity="center"
                            android:paddingStart="10dp"
android:layout_marginBottom="10dp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="14sp" />
                        <Button
                            android:id="@+id/syllabus"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentBottom="true"
                            android:layout_marginBottom="0dp"
                            android:background="#00ff0000"

                            android:padding="20dp"
                            android:textAllCaps="true"
                            android:textColor="@color/blackTextColor" />


                    </RelativeLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:layout_width="150dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_height="150dp"

                        android:background="@drawable/box">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:src="@drawable/resrer"
                            android:padding="5dp">

                        </ImageView>




                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/capriola"
                            android:padding="10dp"
                            android:layout_alignParentBottom="true"
                            android:paddingStart="20dp"
                            android:text="Check Result"
                            android:gravity="center"

                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                        <Button
                            android:id="@+id/result"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentBottom="true"
                            android:background="#00ff0000"
                            android:padding="20dp"

                            android:textAllCaps="true"
                            android:textColor="@color/blackTextColor" />


                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/box">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:src="@drawable/ad"
                            android:padding="5dp">

                        </ImageView>




                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/capriola"
                            android:padding="10dp"
                            android:layout_alignParentBottom="true"
                            android:paddingStart="40dp"
                            android:text="Admission"
                            android:gravity="center"

                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                        <Button
                            android:id="@+id/admission"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentBottom="true"
                            android:background="#00ff0000"
                            android:padding="20dp"

                            android:textAllCaps="true"
                            android:textColor="@color/blackTextColor" />


                    </RelativeLayout>


                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"

                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="20dp"
                        android:background="@drawable/box">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:src="@drawable/web"
                            android:padding="5dp">

                        </ImageView>




                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/capriola"
                            android:padding="10dp"
                            android:layout_alignParentBottom="true"
                            android:paddingStart="20dp"
                            android:text="Visit Website"
                            android:gravity="center"

                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                        <Button
                            android:id="@+id/kea_website"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentBottom="true"
                            android:background="#00ff0000"
                            android:padding="20dp"

                            android:textAllCaps="true"
                            android:textColor="@color/blackTextColor" />


                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="25dp"
                        android:background="@drawable/box">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="100dp"
                            android:src="@drawable/about"
                            android:padding="5dp">

                        </ImageView>




                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/capriola"
                            android:padding="10dp"
                            android:layout_alignParentBottom="true"
                            android:text="About Kcet"
                            android:gravity="center"
                            android:layout_marginStart="20dp"

                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                        <Button
                            android:id="@+id/about"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentBottom="true"
                            android:background="#00ff0000"
                            android:padding="20dp"

                            android:textAllCaps="true"
                            android:textColor="@color/blackTextColor" />


                    </RelativeLayout>


                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content"/>

            </LinearLayout>
        </ScrollView>


    </LinearLayout>



</LinearLayout>